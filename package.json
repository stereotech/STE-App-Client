{
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NUXT_ENV_PLATFORM=WEB nuxt",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "build-for-mobile": "cross-env NUXT_ENV_PLATFORM=MOBILE nuxt build --spa",
    "build-for-panel": "cross-env NUXT_ENV_PLATFORM=PANEL nuxt build --spa",
    "build-for-web": "cross-env NUXT_ENV_PLATFORM=WEB nuxt build --spa",
    "build-web": "npm run build-for-web",
    "build-mobile": "npm run build-for-mobile && npm run replace-relative-paths && npm run copy-cordova && npm run build-android",
    "build-panel": "npm run build-for-panel && npm run replace-relative-paths && npm run copy-cordova && npm run build-electron",
    "build-android": "cd cordova && cordova prepare android",
    "build-electron": "cd cordova && cordova build electron",
    "replace-relative-paths": "node replace-in-files.js",
    "copy-cordova": "cp -R dist/* cordova/www/",
    "clear-cordova": "rm -R cordova/www/*"
  },
  "dependencies": {
    "@aspnet/signalr": "^1.1.4",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/pwa": "^3.0.0-beta.16",
    "nuxt": "^2.8.1",
    "nuxt-class-component": "^1.2.1",
    "nuxt-property-decorator": "^1.3.1",
    "simple-keyboard": "^2.24.1",
    "typeface-roboto": "0.0.75",
    "vue-filter-pretty-bytes": "^0.1.2",
    "vue-moment": "^4.0.0",
    "vue2-filters": "^0.4.1",
    "vuetify": "^2.0.0",
    "vuetify-loader": "^1.1.1",
    "vuex-class": "^0.3.1"
  },
  "devDependencies": {
    "@mdi/font": "^3.9.97",
    "@nuxt/typescript": "^2.8.1",
    "@nuxtjs/proxy": "^1.3.3",
    "@nuxtjs/vuetify": "^1.0.0",
    "@types/node": "^10.12.18",
    "cross-env": "^5.2.0",
    "deepmerge": "^4.0.0",
    "fibers": "^4.0.1",
    "replace": "^1.1.0",
    "sass": "^1.22.7",
    "sass-loader": "^7.1.0",
    "ts-node": "^8.3.0",
    "tslint-config-standard": "^8.0.1"
  }
}