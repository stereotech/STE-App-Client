{
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build-web": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "tslint --project .",
    "dev-debug": "node --inspect node_modules/nuxt-ts-edge/bin/nuxt-ts",
    "build-mobile": "npm run build-web && npm run replace-relative-paths && npm run copy-cordova && npm run run-android",
    "debug-cordova": "npm run build-web && npm run replace-relative-paths && npm run clear-cordova && npm run copy-cordova && npm run run-browser",
    "replace-relative-paths": "node replace-in-files.js",
    "copy-cordova": "cp -R dist/* cordova/www/",
    "clear-cordova": "rm -R cordova/www/*",
    "run-android": "cd cordova && cordova run android",
    "run-browser": "cd cordova && cordova serve"
  },
  "dependencies": {
    "@aspnet/signalr": "^1.1.4",
    "@mdi/font": "^3.3.92",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.3.0",
    "@nuxtjs/pwa": "^3.0.0-beta.16",
    "nuxt": "^2.5.1",
    "nuxt-class-component": "^1.2.1",
    "nuxt-property-decorator": "^1.3.1",
    "vue-filter-pretty-bytes": "^0.1.2",
    "vue-moment": "^4.0.0",
    "vue2-filters": "^0.4.1",
    "vuetify": "^2.0.0",
    "vuetify-loader": "^1.1.1",
    "vuex-class": "^0.3.1",
    "vuex-persistedstate": "^2.5.4"
  },
  "devDependencies": {
    "@mdi/font": "^3.3.92",
    "@nuxt/typescript": "^2.5.1",
    "@nuxtjs/proxy": "^1.3.3",
    "@nuxtjs/vuetify": "^1.0.0",
    "@types/node": "^10.12.18",
    "deepmerge": "^4.0.0",
    "fibers": "^4.0.1",
    "replace": "^1.1.0",
    "sass": "^1.22.7",
    "sass-loader": "^7.1.0",
    "tslint-config-standard": "^8.0.1"
  }
}