{
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NUXT_ENV_PLATFORM=WEB nuxt-ts",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "build-for-mobile": "cross-env NUXT_ENV_PLATFORM=MOBILE nuxt-ts build --spa",
    "build-for-panel": "cross-env NUXT_ENV_PLATFORM=PANEL nuxt-ts build --spa",
    "build-for-web": "cross-env NUXT_ENV_PLATFORM=WEB nuxt-ts build --spa",
    "build-web": "npm run build-for-web",
    "build-android": "npm run build-for-mobile && npm run replace-relative-paths && npm run copy-cordova",
    "build-panel": "npm run build-for-panel && npm run replace-relative-paths && npm run copy-cordova && npm run build-electron",
    "build-for-android": "cd cordova && cordova build android --release",
    "build-electron": "cd cordova && cordova build electron --release",
    "replace-relative-paths": "node replace-in-files.js",
    "copy-cordova": "cp -R dist/* cordova/www/",
    "clear-cordova": "rm -R cordova/www/*"
  },
  "dependencies": {
    "@aspnet/signalr": "^1.1.4",
    "@nuxt/typescript-runtime": "^0.4.10",
    "@nuxtjs/axios": "^5.11.0",
    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "cordova-set-version": "^11.0.1",
    "moment": "^2.27.0",
    "nuxt": "^2.14.6",
    "nuxt-class-component": "^1.2.1",
    "nuxt-i18n": "^6.15.4",
    "nuxt-property-decorator": "^1.3.1",
    "simple-keyboard": "^2.32.100",
    "typeface-roboto": "0.0.75",
    "vue-filter-pretty-bytes": "^0.1.6",
    "vue-i18n": "^8.22.4",
    "vue2-filters": "^0.11.0",
    "vuetify": "^2.3.15",
    "vuetify-loader": "^1.1.1",
    "vuex-class": "^0.3.1"
  },
  "devDependencies": {
    "@mdi/font": "^5.8.55",
    "@nuxt/typescript-build": "^1.0.3",
    "@nuxtjs/eslint-config-typescript": "^0.1.3",
    "@nuxtjs/moment": "^1.6.0",
    "@nuxtjs/proxy": "^1.3.3",
    "@nuxtjs/vuetify": "^1.11.2",
    "@types/node": "^14.14.3",
    "cross-env": "^5.2.0",
    "deepmerge": "^4.0.0",
    "eslint": "^6.6.0",
    "eslint-plugin-vue": "^5.2.3",
    "eslint-plugin-vuetify": "^1.0.0-beta.6",
    "fibers": "^4.0.1",
    "replace": "^1.2.0",
    "sass": "^1.27.0",
    "sass-loader": "^10.0.4",
    "ts-node": "^8.3.0",
    "tslint-config-standard": "^8.0.1"
  }
}